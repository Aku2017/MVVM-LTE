<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>MVVM.LTE</PackageId>
    <Authors>John Ansa</Authors>
    <Company>Bloggrammer.com</Company>
    <Description>MVVM.LTE is a lightweight .NET MVVM library for building loosely coupled, maintainable, and testable MVVM applications.</Description>
    <PackageTags>mvvm wpf C# prism ioc</PackageTags>
    <PackageProjectUrl>https://github.com/Blogrammer/MVVM-LTE/blob/main/README.md</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Blogrammer/MVVM-LTE</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <Copyright>Copyright © 2021 John Ansa</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Version>1.0.1</Version>
  </PropertyGroup>

</Project>
